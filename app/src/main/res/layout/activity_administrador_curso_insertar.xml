<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Vista.Administrador.Curso.InsertarCurso">

    <androidx.appcompat.widget.Toolbar
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/toolbar_curso_insertar_administrador"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="16dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/tv_agregar_curso_codigo" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="1dp" android:layout_marginEnd="1dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_agregar_curso_codigo"
            android:minHeight="48dp"
            tools:ignore="SpeakableTextPresentCheck"
            android:hint="Codigo"
            app:layout_constraintTop_toBottomOf="@+id/toolbar_curso_insertar_administrador"
            android:layout_marginBottom="16dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/tv_agregar_curso_nombre"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="39dp" android:layout_marginEnd="39dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:minHeight="48dp"
            tools:ignore="SpeakableTextPresentCheck"
            android:id="@+id/tv_agregar_curso_nombre"
            android:hint="Nombre"
            app:layout_constraintTop_toBottomOf="@+id/tv_agregar_curso_codigo" android:layout_marginBottom="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/tv_agregar_curso_creditos"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="41dp" android:layout_marginEnd="41dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/tv_agregar_curso_creditos"
            tools:ignore="SpeakableTextPresentCheck"
            android:minHeight="48dp"
            android:hint="Creditos"
            app:layout_constraintTop_toBottomOf="@+id/tv_agregar_curso_nombre" android:layout_marginBottom="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/tv_agregar_curso_a単o"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="43dp" android:layout_marginEnd="43dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_agregar_curso_a単o"
            android:hint="A単o"
            app:layout_constraintTop_toBottomOf="@+id/tv_agregar_curso_creditos" android:layout_marginBottom="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/tv_agregar_curso_ciclo"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="39dp" android:layout_marginEnd="39dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_agregar_curso_ciclo"
            android:hint="Ciclo"
            app:layout_constraintTop_toBottomOf="@+id/tv_agregar_curso_a単o" android:layout_marginBottom="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/tv_agregar_curso_cantidadGrupos"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="44dp" android:layout_marginEnd="44dp"/>
    <Button
            android:text="Insertar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnInsertar_cursoActualizar"
            app:layout_constraintTop_toBottomOf="@+id/tv_agregar_curso_cantidadGrupos"
            app:layout_constraintEnd_toStartOf="@+id/btnCancelar_cursoActualizar" android:layout_marginBottom="105dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="1dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="Cancelar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnCancelar_cursoActualizar"
            app:layout_constraintBaseline_toBaselineOf="@+id/btnInsertar_cursoActualizar"
            app:layout_constraintStart_toEndOf="@+id/btnInsertar_cursoActualizar"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_agregar_curso_cantidadGrupos"
            android:hint="Cantidad de Grupos"
            app:layout_constraintTop_toBottomOf="@+id/tv_agregar_curso_ciclo"
            android:layout_marginBottom="96dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/btnInsertar_cursoActualizar"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="50dp" android:layout_marginEnd="50dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>