<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Vista.Administrador.Factura.InsertarProfesor">

    <androidx.appcompat.widget.Toolbar
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize" android:id="@+id/toolbar_profesor_insertar_administrador"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="1dp" android:layout_marginEnd="1dp"
    />

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_id_factura"
            android:minHeight="48dp"
            tools:ignore="SpeakableTextPresentCheck"
            android:hint="id"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="11dp"
            app:layout_constraintTop_toBottomOf="@+id/toolbar_profesor_insertar_administrador"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_fecha_factura"
            android:minHeight="48dp"
            tools:ignore="SpeakableTextPresentCheck"
            android:hint="Fecha"
            app:layout_constraintStart_toStartOf="@+id/tv_id_factura" android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_id_factura"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/tv_moneda_factura"
            tools:ignore="SpeakableTextPresentCheck"
            android:minHeight="48dp"
            android:hint="moneda"
            app:layout_constraintStart_toStartOf="@+id/tv_tipo_factura" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_tipo_factura"/>

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_tipo_factura"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" android:hint="Tipo"
            app:layout_constraintStart_toStartOf="@+id/tv_fecha_factura" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_fecha_factura"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_cantidad_factura"
            android:hint="cantidad"
            app:layout_constraintStart_toStartOf="@+id/tv_moneda_factura" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_moneda_factura"/>

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_descripcion_factura"
            android:hint="Descripcion"
            app:layout_constraintStart_toStartOf="@+id/txt_cantidad_factura" android:layout_marginTop="11dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_cantidad_factura"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_precioUnitario_factura"
            android:hint="Precio Unitario"
            app:layout_constraintStart_toStartOf="@+id/txt_descripcion_factura" android:layout_marginTop="11dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_descripcion_factura"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_importe_factura"
            android:hint="importe"
            app:layout_constraintStart_toStartOf="@+id/txt_precioUnitario_factura" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_precioUnitario_factura"/>
    <Button
            android:text="Insertar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnInsertarProfesor"
            app:layout_constraintTop_toTopOf="@+id/btnCancelarProfesor" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/btnCancelarProfesor"
            android:layout_marginStart="48dp"/>
    <Button
            android:text="Cancelar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnCancelarProfesor"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="60dp"
            android:layout_marginBottom="83dp"
            app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>