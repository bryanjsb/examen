<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Vista.Administrador.Cliente.InsertarEstudiante">

    <androidx.appcompat.widget.Toolbar
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/toolbar_estudiante_insertar_administrador"
            android:layout_marginTop="2dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="1dp" android:layout_marginEnd="1dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_cedula_estudiante_admin"
            android:minHeight="48dp"
            tools:ignore="SpeakableTextPresentCheck"
            android:hint="Cédula"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="29dp"
            app:layout_constraintTop_toBottomOf="@+id/toolbar_estudiante_insertar_administrador"
            app:layout_constraintEnd_toEndOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_nombre_estudiante_admin"
            android:minHeight="48dp"
            tools:ignore="SpeakableTextPresentCheck"
            android:hint="Nombre"
            app:layout_constraintStart_toStartOf="@+id/tv_cedula_estudiante_admin" android:layout_marginTop="27dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_cedula_estudiante_admin"/>
    <Button
            android:text="Insertar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnInsertaEstudianteAdmin"
            app:layout_constraintEnd_toStartOf="@+id/btnCancelarEstudianteAdmin" android:layout_marginBottom="33dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="7dp"/>
    <Button
            android:text="Cancelar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnCancelarEstudianteAdmin"
            app:layout_constraintStart_toEndOf="@+id/btnInsertaEstudianteAdmin"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="3dp"
    />
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_email_estudiante_admin"
            android:hint="Email"
            tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintStart_toStartOf="@+id/tv_telefono_estudiante_admin" android:layout_marginTop="27dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_telefono_estudiante_admin"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_telefono_estudiante_admin"
            android:hint="Teléfono" tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintStart_toStartOf="@+id/tv_nombre_estudiante_admin" android:layout_marginTop="27dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_nombre_estudiante_admin"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/tv_direccion_estudiante_admin"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"
            android:hint="Direccion"
            app:layout_constraintStart_toStartOf="@+id/tv_email_estudiante_admin" android:layout_marginTop="27dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_email_estudiante_admin"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/tv_celular_estudiante_admin"
            android:hint="Celular" tools:ignore="TouchTargetSizeCheck"
            app:layout_constraintStart_toStartOf="@+id/tv_direccion_estudiante_admin" android:layout_marginTop="28dp"
            android:layout_marginStart="6dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_direccion_estudiante_admin"/>
</androidx.constraintlayout.widget.ConstraintLayout>